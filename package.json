{
  "name": "template-npm",
  "version": "1.0.0",
  "description": "npm package template.",
  "main": "dist/prod/index.js",
  "typings": "dist/prod/index.d.ts",
  "keywords": [
    "template",
    "typescript",
    "mocha",
    "chai",
    "istanbul",
    "travis-ci",
    "appveyor",
    "codecov",
    "coverage"
  ],
  "scripts": {
    "_mocha_": "mocha",
    "_istanbul_": "istanbul cover -x **/*.test.js _mocha",
    "_remap_": "remap-istanbul -i ./dist/coverage/coverage.json -o ./dist/coverage/coverage.json",
    "_remap-html_": "remap-istanbul -i ./dist/coverage/coverage.json -o ./dist/html-report -t html",
    "_open-html_": "open ./dist/html-report/index.html",
    "_publish:prod_": "npm publish ./",
    "_publish:beta_": "npm publish ./ --tag beta",
    "clean": "rimraf dist",
    "watch": "mocha-typescript-watch",
    "lint": "tslint -p ./",
    "build:dev": "tsc",
    "build:prod": "tsc -p tsconfig.prod.json",
    "rewatch": "npm-run-all clean watch",
    "rebuild:dev": "npm-run-all clean build:dev",
    "rebuild:prod": "npm-run-all clean build:prod",
    "test": "npm-run-all clean build:dev _mocha_",
    "cov": "npm-run-all clean build:dev _istanbul_ _remap_",
    "report": "npm-run-all build:dev _istanbul_ _remap-html_ _open-html_",
    "publish:prod": "npm-run-all rebuild:prod _publish:prod_",
    "publish:beta": "npm-run-all rebuild:prod _publish:beta_"
  },
  "author": "Harurow (https://github.com/Harurow)",
  "license": "CC0-1.0",
  "repository": "https://github.com/Harurow/template-npm.git",
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/mocha": "^2.2.41",
    "chai": "^4.0.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "mocha-typescript": "^1.1.7",
    "npm-run-all": "^4.0.2",
    "remap-istanbul": "^0.9.5",
    "rimraf": "^2.6.1",
    "source-map-support": "^0.4.15",
    "tslint": "^5.4.3",
    "typescript": "^2.4.1"
  }
}
